<%= render 'layouts/admin_header' %>
<h1>新規投稿</h1>
<!-- form_with部分 -->
<%= form_with model: @item, url:'/admins/items', local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>タイトル</h4>
  <%= f.text_field :name %>
  <h4>本文</h4>
  <%= f.text_area :introduction %>
  <%= f.text_field :price %>
  <%= f.submit '投稿' %>
<% end %>